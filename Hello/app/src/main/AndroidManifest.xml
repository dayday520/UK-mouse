<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.administrator.hello" >

    <application
        android:allowBackup="true"
        android:theme="@style/AppTheme"
        android:icon="@drawable/icon1"
        android:label="@string/app_name">


        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".EasyActivity"
            android:label="">
            <intent-filter>
                <action android:name="com.litreily.Easymode"/>
                <category android:name="android.intent.category.DEFAULT"/>
            </intent-filter>
        </activity>

        <activity
            android:name=".EntertainActivity"
            android:label="">
            <intent-filter>
                <action android:name="com.litreily.Entertain"/>
                <category android:name="android.intent.category.DEFAULT"/>
            </intent-filter>
        </activity>

        <activity
            android:name=".HardActivity"
            android:label="">
            <intent-filter>
                <action android:name="com.litreily.Hardmode"/>
                <category android:name="android.intent.category.DEFAULT"/>
            </intent-filter>
        </activity>

        <activity
            android:name=".VideoplayActivity"
            android:label="">
            <intent-filter>
                <action android:name="com.litreily.Videoplay"/>
                <category android:name="android.intent.category.DEFAULT"/>
            </intent-filter>
        </activity>

    </application>

</manifest>
